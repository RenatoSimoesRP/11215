<thunder_skin>
	<info>
		<name>default</name>
		<author>Thunder</author>
		<copyright>Thunder</copyright>
		<date></date>
	</info>

	<skin name="default" id="default" type="full">

		<respackage></respackage>

		<bkg>
			<image>defaultbkg.jpg</image>
		</bkg>
		<default_color_scheme>blue</default_color_scheme>
		
		<ani>
			<flash>
				<loop>true</loop>
				<left>0</left>
				<top>0</top>
				<width>1280</width>
				<height>140</height>
				<src>flash\center.swf</src>
				<center>true</center>
				<framelimit>24</framelimit>
				<flashinzip>false</flashinzip>
			</flash>
		</ani>

		<thumbnail>default.png</thumbnail>

	</skin>

	<skin id="default.blue" name="blue" type="bkg">
		<bkg>
			<image>blue\bkg.jpg</image>
		</bkg>
		<thumbnail>blue\thumbnail.jpg</thumbnail>
		<owner_skin>default</owner_skin>
		<default_color_scheme>blue</default_color_scheme>
	</skin>

